{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {
    "eofline": true
  },
  "rules": {
    "trailing-comma": [false],
    "member-ordering": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "import-name": [false]
  },
  "rulesDirectory": []
}
